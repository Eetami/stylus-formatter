{
  "name": "whitespace-formatter",
  "publisher": "eetami",
  "version": "0.0.3",
  "displayName": "Whitespace Formatter",
  "description": "Formatter for all files",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#9FB3D0",
    "theme": "light"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eetami/whitespace-formatter"
  },
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "keywords": [
    "format",
    "newline",
    "tabs",
    "trim",
    "whitespace"
  ],
  "preview": true,
  "activationEvents": [
    "*"
  ],
  "main": "build/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "Whitespace Formatter configuration",
        "properties": {
          "whitespace.format.enable": {
            "type": "boolean",
            "default": true,
            "description": "Enable Whitespace Formatter"
          }
        }
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "babel src --out-dir build",
    "build:dev": "babel src --out-dir build --source-maps",
    "build:watch": "babel src --watch --out-dir build --source-maps",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "source-map": "^0.7.3",
    "typescript": "^3.2.4",
    "vscode": "^1.1.25"
  }
}
