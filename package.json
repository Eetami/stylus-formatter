{
  "name": "stylus-formatter",
  "displayName": "Stylus Formatter",
  "description": "Formatter for .styl files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:stylus"
  ],
  "main": "build/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "stylus",
        "extensions": [
          ".styl"
        ],
        "aliases": [
          "Stylus",
          "lang-stylus",
          "stylus-lang"
        ]
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "babel src --out-dir build --source-maps",
    "build:watch": "babel src --watch --out-dir build --source-maps",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "lodash": "^4.17.11",
    "source-map": "^0.7.3",
    "typescript": "^3.2.4",
    "vscode": "^1.1.25"
  }
}
